<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="shahid_theme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_color</item>
        <item name="colorPrimaryVariant">@color/primary_color</item>//dark color
        <item name="colorOnPrimary">@color/primary_color</item>
        <item name="windowNoTitle">true</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/assent_color</item>
        <item name="colorSecondaryVariant">@color/assent_color</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="radio_button_style" parent="shahid_theme">
        <item name="colorControlNormal">@color/mid_white</item>
        <item name="colorControlActivated">@color/assent_color</item>

    </style>


</resources>